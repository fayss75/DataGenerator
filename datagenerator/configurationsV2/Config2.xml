<dataconfig name="Config2">

	<mainGenerators name="skuV1" />

	<dataGenerator name="skuV1" class="fr.fayss.datagenerator.atg.RepositoryItemGenerator">
		<propertyConfig name="itemName">sku</propertyConfig>
		<propertyConfig name="idGenerator">
			<propertyConfig name="prefix">sku</propertyConfig>
		</propertyConfig>
		<propertyConfig name="propertyList" type="list">
			<dataGeneratorRef>integerProperty</dataGeneratorRef>
			<dataGeneratorRef>stringProperty</dataGeneratorRef>
			<dataGeneratorRef>integerProperty</dataGeneratorRef>
		</propertyConfig>
	</dataGenerator>

	<dataGenerator name="integerProperty" class="fr.fayss.datagenerator.atg.PropertyGenerator">
		<propertyConfig name="dataTypeGenerator">
			<dataGeneratorRef>integerGenerator</dataGeneratorRef>
		</propertyConfig>
		<propertyConfig name="propertyName">integerProperty</propertyConfig>
	</dataGenerator>

	<dataGenerator name="stringProperty" class="fr.fayss.datagenerator.atg.PropertyGenerator">
		<propertyConfig name="dataTypeGenerator">
			<dataGeneratorRef>stringGenerator</dataGeneratorRef>
		</propertyConfig>
		<propertyConfig name="propertyName">stringProperty</propertyConfig>
	</dataGenerator>


	<dataGenerator name="stringGenerator" class="fr.fayss.datagenerator.types.StringGenerator"/>

	<dataGenerator name="integerGenerator" class="fr.fayss.datagenerator.types.IntegerGenerator"/>

</dataconfig>